<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  
  <display-name>Proyecto Disquera</display-name>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>controller.Sgenero</servlet-name>
    <servlet-class>controller.Sgenero</servlet-class>
  </servlet>

  <servlet-mapping> 
    <servlet-name>controller.Sgenero</servlet-name>
    <url-pattern>/genero</url-pattern>
  </servlet-mapping>

  <!-- SERVLET USUARIO DAVIPLATA -->

<servlet>
  <servlet-name>controller.Usuario</servlet-name>
  <servlet-class>controller.Usuario</servlet-class>
</servlet>

<servlet-mapping> 
  <servlet-name>controller.Usuario</servlet-name>
  <url-pattern>/usuario</url-pattern>
</servlet-mapping>


  <!-- SERVLET USUARIO NEQUI -->

<servlet>
  <servlet-name>controller.UsuarioNequi</servlet-name>
  <servlet-class>controller.UsuarioNequi</servlet-class>
</servlet>

<servlet-mapping> 
  <servlet-name>controller.UsuarioNequi</servlet-name>
  <url-pattern>/usuarionequi</url-pattern>
</servlet-mapping>

  <!-- SERVLET BOLSILLO -->

<servlet>
  <servlet-name>controller.Bolsillo</servlet-name>
  <servlet-class>controller.Bolsillo</servlet-class>
</servlet>

<servlet-mapping> 
  <servlet-name>controller.Bolsillo</servlet-name>
  <url-pattern>/bolsillo</url-pattern>
</servlet-mapping>

</web-app>
